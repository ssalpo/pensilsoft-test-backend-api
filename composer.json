{
  "name": "ssalpo/pensilsoft-test-backend-api",
  "type": "framework",
  "require": {
    "php": ">=8.0",
    "vlucas/phpdotenv": "^5.5",
    "nesbot/carbon": "^2.0",
    "illuminate/database": "^8.0",
    "illuminate/events": "^8.0",
    "nikic/fast-route": "^1.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.0",
    "mockery/mockery": "^1.3"
  },
  "autoload": {
    "psr-4": {
      "App\\": "App/",
      "Core\\": "Core/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
